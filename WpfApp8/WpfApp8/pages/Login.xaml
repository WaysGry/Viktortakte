        <Button Content="Регистрация" HorizontalAlignment="Left" Height="24" Margin="456,300,0,0" VerticalAlignment="Top" Width="131" Click="Button_Click_1"/>
        <Label Content="Login" HorizontalAlignment="Left" Height="32" Margin="467,151,0,0" VerticalAlignment="Top" Width="80" FontFamily="Showcard Gothic"/>
        <Label Content="Password" HorizontalAlignment="Left" Height="23" Margin="467,214,0,0" VerticalAlignment="Top" Width="80" FontFamily="Showcard Gothic"/>
        <Label Content="VIKTORTAKTE" HorizontalAlignment="Left" Height="98" Margin="247,25,0,0" VerticalAlignment="Top" Width="340" FontSize="48" FontFamily="Showcard Gothic"/>

    </Grid>
</Page>
